<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<div class="delete-friend-modal-section d-none">
    <div class="delete-friend-modal-box">
	    <div class="modal-title">친구</div>
	    <hr class="m-0">
    	<div>
	    	<button class="delete-friend-btn text-danger">친구 끊기</button>
    	</div>
    	<hr class="m-0">
    	<div>
	    	<button class="cancel-delete-friend-modal">취소</button>
    	</div>
    </div>
</div>

<div class="request-friend-modal-section d-none">
    <div class="request-friend-modal-box">
	    <div class="modal-title">친구 요청 취소</div>
	    <hr class="m-0">
    	<div>
	    	<button class="request-cancel-friend-btn text-danger">친구 요청 취소</button>
    	</div>
    	<hr class="m-0">
    	<div>
	    	<button class="cancel-request-friend-modal">취소</button>
    	</div>
    </div>
</div>

<div class="response-friend-modal-section d-none">
    <div class="response-friend-modal-box">
	    <div class="modal-title">친구 요청 응답</div>
	    <hr class="m-0">
    	<div>
	    	<button class="response-friend-action-btn text-primary">친구 요청 수락</button>
    	</div>
    	<hr class="m-0">
    	<div>
	    	<button class="refuse-friend-action-btn text-danger">친구 요청 거절</button>
    	</div>
    	<hr class="m-0">
    	<div>
	    	<button class="cancel-response-friend-modal">취소</button>
    	</div>
    </div>
</div>

